Cell["\<\
Define a utility function to easily extract a failed test from the test \
results.\
\>", "Text",
 CellChangeTimes->{{3.816659195559901*^9, 
  3.816659228602191*^9}},ExpressionUUID->"b4bb07a5-fe72-4516-988a-\
ec4ab0ca2bee"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"GetFailedTest", "[", "testsuitereport_", "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"testsFailed", "=", 
       RowBox[{"Cases", "[", 
        RowBox[{
         RowBox[{"testsuitereport", "[", "\"\<TestsFailed\>\"", "]"}], ",", 
         "_TestResultObject", ",", 
         RowBox[{"-", "1"}]}], "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
     "testsFailed"}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GetFailedTest", "[", 
   RowBox[{"testsuitereport_", ",", "index_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "failedTests", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"failedTests", "=", 
      RowBox[{"GetFailedTest", "[", "testsuitereport", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Max", "[", "index", "]"}], ">", 
        RowBox[{"Length", "[", "failedTests", "]"}]}], ",", 
       RowBox[{"Return", "[", 
        RowBox[{"$Failed", ",", "Module"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"failedTests", "[", 
      RowBox[{"[", "index", "]"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
 CellLabel->"In[4]:=",ExpressionUUID->"e7f887ee-5d1f-4df3-80f3-55dfa0ac0f2f"],

